  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-core": "7.0.0-bridge.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-plugin-html": "^5.0.0",
    "node-sass": "^4.10.0",
    "parcel-bundler": "1.10.3",
